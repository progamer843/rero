function humanbarrage()
	if death == true then return end
	if active == true then return end
	active = true
	local bep = true
	local de = mouse.KeyUp:connect(function(key)
	    if key == "e" then
	      bep = false
	    end
	end)
	_G.anim29:Play(0.1,1,999)
	repeat
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.5,0.1,-0.5), Vector3.new(-0.5,0.1,-0.5), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.3,0.1,-0.3), Vector3.new(-0.3,0.1,-0.3), BrickColor.new("Bright green"))
		game:GetService("ReplicatedStorage").PlayerStrongPunch:FireServer(Vector3.new(-0.1,0.1,-0.1), Vector3.new(-0.1,0.1,-0.1), BrickColor.new("Bright green"))
		_G.playerbarrageone()
		_G.playerbarragetwo()
		wait(0.000005)
	until bep == false
	_G.anim29:Stop()
	active = false
end

mouse.KeyDown:connect(function(key)
	if key == "e" then

		humanbarrage()
	end
end)
_G.bringall = function()
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.huge then
                game.ReplicatedStorage.KQRGrab:FireServer(v.Humanoid)
            end
        end
    end
end
