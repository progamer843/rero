local joseph = game:GetService("ReplicatedStorage").JosephHamon.Event
local r = "boom"
local g = "molotov"
local e = "volley"
local c = "rope"
local rope = false
mouse.KeyDown:connect(function(key)
    if key == "=" then
        if rope == false then
            rope = true
            repeat
                wait()
                joseph:FireServer(c)
            until rope == false
        elseif rope == true then
            rope = false
            end
        end
end)
