	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
	        	game.ReplicatedStorage.Damage12Sans:FireServer(v.Humanoid, v.HumanoidRootPart.CFrame, 99, 0, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.LookVector * 0 + Vector3.new(0, 0, 0), 0, "", 1, 1)
            end
        end
    end
end
local megapunchactiveautra = false
local AuraSetter = 0
mouse.KeyDown:connect(function(key)
	if key == "t" then

		active = true
		_G.anim30:Play()
		megapunchactiveautra = false
		coroutine.resume(coroutine.create(function()
		repeat
            AuraSetter = AuraSetter + 1
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*1, math.sin(AuraSetter * 50)*0), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*2, math.sin(AuraSetter * 50)*1), 0, 0, Vector3.new(0,0,0), "rbxassetid://", 3, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-1, math.sin(AuraSetter * 50)*0), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-2, math.sin(AuraSetter * 50)*-2), 0, 0, Vector3.new(0,0,0), "rbxassetid://", 3, Color3.new(0, 255, 0), "", 1, 1)
            game.ReplicatedStorage.Damage:FireServer(human, chr["Right Arm"].CFrame * CFrame.new(math.sin(AuraSetter * 50)*0, math.cos(AuraSetter * 50)*-2, math.sin(AuraSetter * 50)*3), 0, 0, Vector3.new(0,0,0), "rbxassetid://245751634", 3, Color3.new(0, 255, 0), "", 1, 1)
