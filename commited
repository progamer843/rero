_G.humanityDestroy = function()
    fatfuck()
    doppioslam:FireServer()
    game:GetService("ReplicatedStorage").Damage12Sans:FireServer(game.Players.LocalPlayer.Character.Humanoid, game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame, 0, 0, Vector3.new(0, 0, 0), 0, "rbxassetid://3673187353", 2, 10)
    for i,v in pairs(game.Workspace.Entities:GetChildren()) do
        if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Name ~= game.Players.LocalPlayer.Name then
            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.HumanoidRootPart.Position).Magnitude < math.huge then
            game.ReplicatedStorage.HamonSword:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game.ReplicatedStorage.SamuraiDamage2:FireServer(v.Humanoid, 1, math.huge, Vector3.new(0,math.huge,0))
            game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 1.5, "", 1, 1)
                            game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").SPOHRTZDMG:FireServer(v.Humanoid)
                game:GetService("ReplicatedStorage").Damage12:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
                game:GetService("ReplicatedStorage").Damage12:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
for i = 1,9 do
game:GetService("ReplicatedStorage").Damage12:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
game:GetService("ReplicatedStorage").Damage12Enderman:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
game:GetService("ReplicatedStorage").Damage12Sans:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
end
for i = 1,10 do
game:GetService("ReplicatedStorage").ULFDamage:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)

game:GetService("ReplicatedStorage").ULFDamage:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), .5, "", 1, 1)
end
for i = 1,50 do
    game:GetService("ReplicatedStorage").Damage3:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
game:GetService("ReplicatedStorage").Damage2:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
game:GetService("ReplicatedStorage").Damage3:FireServer(v.Humanoid, v.Head.CFrame, 0, 0, Vector3.new(0, 0, 0), 5, "", 1, 1)
end

                            game:GetService("ReplicatedStorage").BurnDamage:FireServer(v.Humanoid,v.HumanoidRootPart.CFrame,math.huge,0,Vector3.new(0,0,0),"rbxassetid://241837157",0,Color3.new(255, 255, 255),"",0,0)

            
            end
        end
    end
end

local nukedestroy = false

mouse.KeyDown:connect(function(key)
	if key == "b" then

        nukedestroy = true
		human.WalkSpeed = 0
		human.JumpPower = 0
		_G.anim25:Play()
		wait(0.2)
		_G.humanityDestroy()
		wait(0.35)
		human.WalkSpeed = 20
		human.JumpPower = 50
		wait(1)
        nukedestroy = false
	end
end)
function FixGod()
	if human.Health < 500 then
		game.ReplicatedStorage.SamuraiDamage2:FireServer(human,-math.huge)
		game.ReplicatedStorage.SamuraiDamage2:FireServer(human,math.huge)
	end
end

coroutine.resume(coroutine.create(function()
while human.Health > 0.001 do
	wait(1)
	FixGod()
end
end))

local GetUp = false

coroutine.resume(coroutine.create(function()
while human.Health > 0.01 do
	wait()
	if human.PlatformStand == true and GetUp == false then
		game:GetService("ReplicatedStorage").GetUp:FireServer()
		human:SetStateEnabled(3, true)
		human.PlatformStand = false
		human.AutoRotate = true
		GetUp = true
	end
	if math.random(1, 4) == 1 then
		GetUp = false
	end
end
end))
local deathevent = replicatedstorage.Death
human.HealthChanged:connect(function()
	if human.Health < 1 then
		human:SetStateEnabled(3, false)
		human:SetStateEnabled(15, false)
		attackmode = false
		death = true
		player.ChatColor.G.Value = 0
		player.ChatColor.R.Value = 255
		player.ChatColor.B.Value = 0
		player.ChatColor.BlackWhite.Value = true
		print(attackmode)
		print(human.MaxHealth)
		deathevent:FireServer(false)
	end
end)
